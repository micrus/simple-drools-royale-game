package com.sample


 rule "MOVE NPG"
 salience npc.getStat(StatAbility.SPEED) * 100
 
    when
    	Settings( gameOver == false )
		npc : NPC( status == Status.NOT_MOVED)
		
    then
       System.out.println("npg move! " + npc.toString() );
       npc.setStatus(Status.MOVED);
       update(npc);

 end
 
